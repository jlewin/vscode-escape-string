{
  "name": "escape-string",
  "displayName": "Escape String",
  "description": "Escape string when pasting",
  "version": "1.1.0",
  "publisher": "cvbge",
  "repository": {
    "type": "git",
    "url": "https://github.com/cvbge/vscode-escape-string.git"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:escape-string.pasteEscapedString"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "escape-string.pasteEscapedString",
        "title": "Paste escaped string"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == json || resourceLangId == csharp || resourceLangId == javascript || resourceLangId == typescript || resourceLangId == cpp || resourceLangId == c || resourceLangId == python",
          "command": "escape-string.pasteEscapedString",
          "group": "9_cutcopypaste@3"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.51.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  }
}
